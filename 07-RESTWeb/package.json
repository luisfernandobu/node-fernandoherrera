    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^30.0.0",
    "prisma": "^6.9.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "dotenv": "^16.5.0",
